[
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "alt+h",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "alt+l",
        "command": "editor.action.goToDeclaration"
    },
    {
        "key": "ctrl+space",
        "command": "editor.action.showHover",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m m",
        "command": "bookmarks.toggle",
    },
    {
        "key": "ctrl+m n",
        "command": "bookmarks.jumpToNext",
    },
    {
        "key": "ctrl+m p",
        "command": "bookmarks.jumpToPrevious",
    },
    {
        "key": "ctrl+m l",
        "command": "bookmarks.listFromAllFiles",
    },
    {
        "key": "ctrl+shift+m",
        "command": "workbench.action.toggleMaximizeEditorGroup"
    },

    // copilot
    {
        "key": "ctrl+shift+alt+t",
        "command": "github.copilot.interactiveEditor.generateTests"
    },
    { 
        "key": "ctrl+shift+m", "command": "github.copilot.terminal.generateCommitMessage"
    },
    { 
        "key": "ctrl+shift+[", "command": "workbench.action.terminal.focusPrevious"
    },
    { 
        "key": "ctrl+shift+]", "command": "workbench.action.terminal.focusNext"
    },
]
